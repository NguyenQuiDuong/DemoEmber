    <script id='modal/first-time-ux.html' type='text/ng-template'>
        <div class='modal-body'>
            <h3>How Codementor Works</h3>

            <div class='row-fluid' ng-show='modal.step==0'>
                <div class='span12 show-box'>
                    <h4>
                        <small>Search Mentors by Categories.</small>
                    </h4>
                    <div class='cover'>
                        <img ng-src='/assets/img_new/search2.gif' width='400'>
                    </div>
                </div>
            </div>
            <div class='row-fluid ng-cloak' ng-show='modal.step==1'>
                <div class='span12 show-box'>
                    <h4>
                        <small>Chat with mentor and "Start Session"</small>
                    </h4>
                    <div class='cover'>
                        <img ng-src='/assets/img_new/chat.gif' width='400'>
                    </div>
                </div>
            </div>
            <div class='dotdot'>
                <span class='dot' ng-class='{active:modal.step==0}' ng-click='modal.step=0'></span>
                <span class='dot' ng-class='{active:modal.step==1}' ng-click='modal.step=1'></span>
            </div>
            <a class='skip' href='#' ng-click='$close()'>
                Skip
            </a>
            <a class='next' href='#' ng-click='modal.step=1' ng-show='modal.step==0'>
                <i class='icon-chevron-right'></i>
            </a>
            <a class='previous ng-cloak' href='#' ng-click='modal.step=0' ng-show='modal.step==1'>
                <i class='icon-chevron-left'></i>
            </a>
        </div>
    </script>
    <script id='modal/first-time-survey.html' type='text/ng-template'>
        <div class='first-time-survey-main'>
            <div class='hd'>
                <h3>
                    Set up your profile
                </h3>

                <div class='step-tab-bar'>
                    <div class='step-ball' ng-class='{active:modal.step==$index, done:modal.ok[$index]}'
                         ng-repeat='i in [1,2,3]'>
                        <div class='ball'>
<span>
  {{i}}
</span>
                            <i class='icon-ok'></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class='bd'>
                <div class='step-contents'>
                    <div class='step-content' ng-show='modal.step==0'>
                        <div class='survey-question'>
                            <h3>
                                Which topics would you need help with?
                            </h3>

                            <div class='selector topics' ng-init='modal.topicInit()'>
                                <div class='option topic' ng-class='{active:topic.selected}'
                                     ng-click='modal.selectTopic(topic)' ng-repeat='topic in modal.topics'>
                                  {{topic.name}}
                                </div>
                            </div>
                            <div class='controls' ng-class='{requiredWarning:requiredWarning}'
                                 ng-controller='CategoryCtrl as cc' ng-init='enableRecommand=false;'>
                                <tags-input min-length='1' ng-model='tags' on-tag-added='cc.onAddTag($tag)'
                                            on-tag-removed='cc.onRemoveTag($tag)' placeholder='Other topics'>
                                    <auto-complete max-results-to-show='3' min-length='1'
                                                   source='cc.loadTags($query)'></auto-complete>
                                </tags-input>

                            </div>
                        </div>
                    </div>
                    <div class='step-content' ng-show='modal.step==1'>
                        <div class='survey-question q2'>
                            <h3>
                                What's your general technical experience level?
                            </h3>

                            <div class='selector'>
                                <div class='option' ng-class='{active:modal.Q2selected==q.id}'
                                     ng-click='modal.selectQ2(q, $event)' ng-repeat='q in modal.Q2'>
                                    <input name='exp_level' ng-click='modal.selectQ2(q, $event)'
                                           ng-model='modal.Q2selected' ng-value='q.id' type='radio'>
                                  {{q.title}}
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class='step-content' ng-show='modal.step==2'>
                        <div class='survey-question q3'>
                            <h3>
                                Why would you like to use Codementor?
                            </h3>

                            <div class='selector'>
                                <div class='option' ng-class='{active:q.selected}' ng-click='modal.selectQ(q, $event)'
                                     ng-repeat='q in modal.Q3'>
                                    <input ng-click='modal.selectQ(q, $event)' ng-model='q.selected' type='checkbox'>
                                  {{q.title}}
                                </div>
                            </div>
                            <div class='other-text'>
                                <textarea ng-model='modal.Q3other' placeholder='Other'></textarea>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class='ft text-right'>
                <button class='btn' ng-click='modal.skip()'>
                    Skip
                </button>
                <button class='btn ng-cloak' ng-click='modal.previous()' ng-show='modal.step&gt;0'>
                    <div class='icon-chevron-left'></div>
                    Previous
                </button>
                <button class='btn' ng-class="{'btn-primary':modal.ok[modal.step]}" ng-click='modal.next()'
                        ng-hide='modal.step==2'>
                    Next
                    <div class='icon-chevron-right'></div>
                </button>
                <button class='btn' ng-class="{'btn-primary':modal.ok[modal.step]}" ng-click='modal.submit()'
                        ng-show='modal.step==2'>
                    Done
                </button>
            </div>
        </div>
    </script>

    <script>
        $("#freeHint").tooltip("hide");
    </script>

