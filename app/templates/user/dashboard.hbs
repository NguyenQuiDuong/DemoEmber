    <source src='https://cdn.codementor.io/assets/notify3-d13cb6c955d36fb8f27984428e7d4ca2.ogg' type='audio/ogg'>
</audio>
<div id='notifications' ng-controller='connectNowNotifyCtrl'>
    <div class='message ng-cloak' data-name='{{snapshot_name}}' ng-if='show'>
        <div style='min-height: 40px;'>
            <img ng-src='{{data.user_image_url}}'>

            <p ng-if="data.type=='new_realtime_lesson'">
                <strong>
                  {{data.user_name}}
                </strong>
                would like to connect with you now {{free_length}}
                <span ng-hide='data.question_is_direct'>
about:
<strong>
  {{data.title}}
</strong>
</span>
            </p>

            <p ng-if="data.type=='reject_realtime_lesson'">
                <a href='{{data.user_url}}'>
                    <strong>
                      {{data.user_name}}
                    </strong>
                </a>
                declined your request
<span ng-hide='data.question_is_direct'>
about:
<strong>
  {{data.title}}
</strong>
</span>
            </p>

            <p ng-if="data.type=='expire_realtime_lesson'">
                <a href='{{data.user_url}}'>
                    <strong>
                      {{data.user_name}}
                    </strong>
                </a>
                may be away at the moment and did not respond to your request
<span ng-hide='data.question_is_direct'>
:
<strong>
  {{data.title}}
</strong>
</span>
            </p>

            <p ng-if="data.type === 'miss_connection'">
                You missed a request from
                <a href='{{data.user_url}}'>
                    <strong>
                      {{data.user_name}}
                    </strong>
                </a>
<span ng-hide='data.question_is_direct'>
about:
<strong>
  {{data.title}}
</strong>
</span>
            </p>

            <p ng-if="data.type === 'in_prepare_lesson'">
                <a href='{{data.user_url}}'>
                    <strong>
                      {{data.user_name}}
                    </strong>
                </a>
                is waiting to start the session with you
<span ng-hide='data.question_is_direct'>
about:
<strong>
  {{data.title}}
</strong>
</span>
            </p>

            <p ng-if="data.type === 'ongoing_session'">
                You have an ongoing session with
                <strong>
                  {{data.opposite_user.display_name}}
                </strong>
<span ng-hide='data.question_is_direct'>
about:
<strong>
  {{data.question.title}}
</strong>
</span>
            </p>
        </div>
        <div class='actions'>
            <a class='btn btn-danger js-dismiss-schedule' href='#' ng-click='dismiss()'
               ng-if="data.type !== 'new_realtime_lesson' &amp;&amp; data.type !== 'ongoing_session'">
                OK
            </a>
            <a class='btn btn-danger js-dismiss-schedule' href='#' ng-click='joinDismiss()'
               ng-if="data.type === 'ongoing_session'">
                OK
            </a>
            <a class='btn btn-danger js-dismiss-schedule' href='#' ng-click='dismiss(data.decline_url)'
               ng-if="data.type === 'new_realtime_lesson'">
                No Thanks
            </a>
            <a class='btn btn-success js-dismiss-schedule' href='#' ng-click='otherMentors(data.find_mentor_url)'
               ng-if="data.type === 'expire_realtime_lesson' || data.type === 'reject_realtime_lesson'">
                Find Another Mentor
            </a>
            <a class='btn btn-success js-dismiss-schedule' href='#' ng-click='startNow(data.accept_url)'
               ng-if="data.type === 'in_prepare_lesson'">
                Start Now
            </a>
            <a class='btn btn-success js-dismiss-schedule' href='#' ng-click='joinSession(data.url)'
               ng-if="data.type === 'ongoing_session'">
                Join Now
            </a>
            <a class='btn btn-success' href='#' ng-click='tryAgain(data.retry_realtime_url)'
               ng-if="data.type === 'expire_realtime_lesson' &amp;&amp; !data.question_is_direct">
                Try Again
            </a>
            <a class='btn btn-success js-accept-schedule' href='#' ng-click='accept(data.accept_url)'
               ng-if="data.type === 'new_realtime_lesson'">
                Accept and Connect
            </a>
            <a class='btn btn-success' href='#' ng-click='toDashboard(data.question_url)'
               ng-if="data.type === 'miss_connection'">
                View Details
            </a>
        </div>
    </div>
</div>


