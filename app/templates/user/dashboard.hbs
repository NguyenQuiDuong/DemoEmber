<script type="text/ng-template" id="/template/direct-longterm-modal.html">
    <form method="POST" action="/applicant_longterm_directs/create" ng-submit="modal.submit($event)"
          style="margin-bottom:0px;">
        <form-authenticity-token></form-authenticity-token>
        <div class="modal-header">
            <h3>Hire {{modal.config.mentorName}} as Your Long-term Mentor</h3>
        </div>
        <div class="modal-body">
            <div class="row-fluid">
                <div class="span12">
                    <input name="mentor_username" type="hidden" value="{{modal.config.mentorUsername}}">

                    <p class="modal-description">{{modal.config.mentorName}} will respond with a proposed monthly rate
                        based on your request.</p>
                </div>
            </div>
            <div class="row-fluid questionBlock">
                <div class="span12">
                    <div class="question">
                        <div class="stepNumber">1</div>
                        <div class="colName">
                            What do you need help with?
                        </div>
                    </div>
                    <div class="indentBlock">
                        <textarea rows="4" class="description span10" ng-class="{inputError:modal.requiredMsgTitle}"
                                  name="description" ng-model="modal.description"
                                  placeholder="e.g. I'm building a mobile iOS app and want to work with you to launch it in 3 months"></textarea>
                    </div>
                </div>
            </div>
            <div class="row-fluid questionBlock">
                <div class="span12">
                    <div class="question">
                        <div class="stepNumber">2</div>
                        <div class="colName">
                            What time would you like to have sessions generally?
                        </div>
                    </div>
