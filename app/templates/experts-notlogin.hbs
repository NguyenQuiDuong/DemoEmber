<script type="text/ng-template" id="/template/direct-longterm-modal.html">
    <form method="POST" action="/applicant_longterm_directs/create" ng-submit="modal.submit($event)"
          style="margin-bottom:0px;">
        <form-authenticity-token></form-authenticity-token>
        <div class="modal-header">
            <h3>Hire {{modal.config.mentorName}} as Your Long-term Mentor</h3>
        </div>
        <div class="modal-body">
            <div class="row-fluid">
                <div class="span12">
                    <input name="mentor_username" type="hidden" value="{{modal.config.mentorUsername}}">

                    <p class="modal-description">{{modal.config.mentorName}} will respond with a proposed monthly rate
                        based on your request.</p>
                </div>
            </div>
            <div class="row-fluid questionBlock">
                <div class="span12">
                    <div class="question">
                        <div class="stepNumber">1</div>
                        <div class="colName">
                            What do you need help with?
                        </div>
                    </div>
                    <div class="indentBlock">
                        <textarea rows="4" class="description span10" ng-class="{inputError:modal.requiredMsgTitle}"
                                  name="description" ng-model="modal.description"
                                  placeholder="e.g. I'm building a mobile iOS app and want to work with you to launch it in 3 months"></textarea>
                    </div>
                </div>
            </div>
            <div class="row-fluid questionBlock">
                <div class="span12">
                    <div class="question">
                        <div class="stepNumber">2</div>
                        <div class="colName">
                            What time would you like to have sessions generally?
                        </div>
                    </div>
                </div>
            </div>
            <div class="row-fluid questionBlock">
                <div class="span12">
                    <div class="question">
                        <div class="stepNumber">3</div>
                        <div class="colName">
                            How many hours would you like to work with {{modal.config.mentorName}} every week?
                        </div>
                    </div>
                    <div class="row-fluid">
                        <div class="span12">
                            <select class="hourSelector" name="acceptable_hours">
                                <option value="1">1 hour per week</option>
                                <option value="2">2 hours per week</option>
                                <option value="3">3 hours per week</option>
                                <option value="4">4 hours per week</option>
                                <option value="5">5 hours per week</option>
                                <option value="6">6 hours per week</option>
                                <option value="7">7 hours per week</option>
                                <option value="8">8 hours per week</option>
                                <option value="9">9 hours per week</option>
                                <option value="10">10 hours per week</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer text-center">
            <small class="red" ng-show="modal.requiredMsgTitle || modal.requiredMsgContent">* required field &nbsp;&nbsp;</small>
            <button class="btn" type="button" ng-click="$dismiss()">
                <i class="icon-remove"></i>
                Cancel
            </button>
            <button type="submit" ng-class="{'btn-primary':!modal.loading}" class="btn submit">
                <i ng-hide="modal.loading" class="icon-ok"></i>
                <img ng-show="modal.loading" src="image/home/loading_w_l.gif" style="width:16px;">
                <span>Submit</span>
            </button>
        </div>
    </form>
</script>

