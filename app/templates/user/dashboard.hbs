<script id='modal/ask-for-setup-payment.html' type='text/ng-template'>
    <div class='modal-body' style='padding:15px 30px;'>
        <legend>
            Before Proceeding...
        </legend>
        <p ng-if='modal.config.realCreditsToSessionTime &gt;= 15'>
            You only have enough credits for a live session with
          {{modal.config.mentor.name}} up to {{modal.config.realCreditsToSessionTime}} minutes.
            If you'd like to have a longer session, please set up your billing info now.
        </p>

        <p ng-if='modal.config.realCreditsToSessionTime &lt; 15'>
            You don't have enough credits for the first 15 minutes with {{modal.config.mentor.name}}.
            In order to start your session, please set up your billing info now.
        </p>

        <p class='red' ng-if='modal.config.cardDeclined'>
            * Your credit card is unauthorized. You may want to set up your billing info now.
        </p>
    </div>
    <div class='modal-footer' ng-if='modal.config.realCreditsToSessionTime &gt;= 15'>
        <a class='btn' ng-click='$close()'>
            Set up billing info
        </a>
        <a class='btn btn-success' ng-click='modal.next()'>
            Proceed Anyway
        </a>
    </div>
    <div class='modal-footer' ng-if='modal.config.realCreditsToSessionTime &lt; 15'>
        <a class='btn' ng-click='$dismiss()'>
            Cancel
        </a>
        <a class='btn btn-success' ng-click='$close()'>
            Set up billing info
        </a>
    </div>
</script>

<script id='modal/pay-with-stripe-bitcoin.html' type='text/ng-template'>
    <div class='modal-body' style='padding:15px 30px;'>
        <legend>
            Pay With Bitcoin
        </legend>
        <table ng-show='receiver &amp;&amp; !receiver.filled &amp;&amp; !errorMsg'>
            <tr>
                <td>{{receiver.bitcoin_amount}} BTC (${{receiver.amount}})</td>
            </tr>
            <tr>
                <td>
                    <input app-select-all-text='' class='span2' style='width: 100%; height: 30px; font-size: 24px;'
                           type='text' value='{{receiver.inbound_address}}'>
                </td>
            </tr>
        </table>
        <div class='loading' ng-show='generatingReceiver'>
            Loading..
        </div>
        <div class='polling' ng-show='pollingReceiver'>
            Polling..
        </div>
        <div class='filled' ng-show='receiver.filled'>
            <p>Thank you! We have receive your bitcoin payment.</p>
